{
    "manifest_version": 2,
    "name": "GitHub Dark Theme Fork",
    "short_name": "Dark Theme",
    "description": "A Dark theme for all of GitHub based on Atom One Dark.",
    "version": "1.1.28",
    "applications": {
        "gecko": {
            "id": "svo393@gmail.com"
        }
    },
    "browser_action": {
        "default_icon": "icons/icon.png",
        "default_popup": "app/popup.html"
    },
    "background": {
        "scripts": ["app/vendor.js", "app/background.js"]
    },
    "content_scripts": [],
    "permissions": ["tabs", "storage", "<all_urls>"],
    "optional_permissions": [],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
